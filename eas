local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()



local Window = OrionLib:MakeWindow({Name = "YeN Anomic", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})






local Player = Window:MakeTab({
	Name = "- Player",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local Combat = Window:MakeTab({
	Name = "- Combat",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local Visuals = Window:MakeTab({
	Name = "- Visuals",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local Teleportation = Window:MakeTab({
	Name = "- Teleportation",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local Vehicle = Window:MakeTab({
	Name = "- Vehicle",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local AutoFarm = Window:MakeTab({
	Name = "- AutoFarm",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local Other = Window:MakeTab({
	Name = "- Other",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})


local ESP = Visuals:AddSection({
	Name = "ESP"
})

local Lighting = Visuals:AddSection({
	Name = "Lighting"
})

local OtherTP = Teleportation:AddSection({
	Name = "Other Teleportation"
})

local Teleports = Teleportation:AddSection({
	Name = "Waypoints"
})

Player:AddSlider({
	Name = "Walkspeed",	
	Min = 16,
	Max = 60,
	Default = 16,
	Color = Color3.fromRGB(0,0,255),
	Increment = 1,
	ValueName = "speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end
})

Player:AddSlider({
	Name = "Jumppower",
	Min = 50,
	Max = 450,
	Default = 50,
	Color = Color3.fromRGB(0,255,0),
	Increment = 1,
	ValueName = "jumppower",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})

Player:AddButton({
	Name = "Noclip",
	Callback = function()
		
	end
})

Player:AddButton({
	Name = "FullBright (Glitchy)",
	Callback = function()
		game.Lighting.Brightness = 20
		game.Lighting.GlobalShadows = false
	end
})

Player:AddButton({
	Name = "Btools",
	Callback = function()
      	loadstring(game:HttpGet(('https://pastebin.com/raw/LETNPL0E')))()
  	end
})

OtherTP:AddTextbox({
	Name = "Teleport To Player",
	Default = "Player's Exact Username",
	TextDisappear = true,
	Callback = function(Value)
		if game.Players:FindFirstChild(Value) then
			local Oplr = game.Players:FindFirstChild(Value)
			local OplrPart = Oplr.Character.HumanoidRootPart
			
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(OplrPart.Position)
		end
	end	  
})



Combat:AddDropdown({
	Name = "Buy Gun (WILL TAKE MONEY)",
	Default = "None",
	Options = {"None","Pistol", "Cleaver","Snubnose","Revolver","Lockpick","Pocket Pistol","Lever Rifle","Navy Revolver","Classic Shotgun","Classic Pistol","Grease Gun"},
	Callback = function(Value)
		local Table2 = {
	["ItemName"] = Value
}

game:GetService("ReplicatedStorage").Events.PurchaseItem:FireServer("PurchaseItem", Table2)
		
	end
})


ESP:AddButton({
	Name = "Update Player Drop ESP",
	Callback = function()
		for i,v in pairs(game.Workspace.Drops:GetChildren()) do
			local Highlight = Instance.new("Highlight")

			Highlight.Parent = v
		end
	end
})

ESP:AddButton({
	Name = "Update ESP",
	Callback = function()
		for i,v in pairs(game.Players:GetChildren()) do
			local ESP = Instance.new("Highlight")

			ESP.Parent = v.Character
		end
	end
})

ESP:AddColorpicker({
	Name = "ESP Color",
	Default = Color3.fromRGB(0, 255, 0),
	Callback = function(Value)
		for i,v in pairs(game.Workspace.PlayerCharacters:GetChildren()) do
			if v:IsA("Highlight") then
				v.FillColor = Value
			end
		end
	end	  
})

Lighting:AddToggle({
	Name = "Shadows",
	Default = false,
	Callback = function(Value)
		game.Lighting.GlobalShadows = Value
	end    
})

Lighting:AddSlider({
	Name = "Brightness",
	Min = 0,
	Max = 150,
	Default = 2,
	Color = Color3.fromRGB(200,165,255),
	Increment = 1,
	ValueName = "Intensity",
	Callback = function(Value)
		game.Lighting.Brightness = Value
	end
})

Lighting:AddColorpicker({
	Name = "Ambient Color",
	Default = Color3.fromRGB(210, 210, 210),
	Callback = function(Value)
		game.Lighting.Ambient = Value
	end	  
})

Combat:AddSlider({
	Name = "Expanded Hitboxes Size",
	Min = 1,
	Max = 10,
	Default = 1,
	Color = Color3.fromRGB(255,0,255),
	Increment = 1,
	ValueName = "Size",
	Callback = function(Value)
		for i,v in pairs(game.Workspace.PlayerCharacters:GetDescendants()) do
			if v.Name == "Head" then
				v.Size = Vector3.new(Value, Value, Value)
			end
		end
	end
})

Combat:AddSlider({
	Name = "Expanded Hitboxes Transparency",	
	Min = 0,
	Max = 1,
	Default = .1,
	Color = Color3.fromRGB(0,0,255),
	Increment = 1,
	ValueName = "Size",
	Callback = function(Value)
		for i,v in pairs(game.Workspace.PlayerCharacters:GetDescendants()) do
			if v.Name == "Head" then
				v.Transparency = Value
			end
		end
	end
})




OtherTP:AddButton({
	Name = "Teleport to Random Player Drop",
	Callback = function()
		for i,v in pairs(game.Workspace.Drops:GetChildren()) do
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.MainPart.Position)
		end
	end
})

