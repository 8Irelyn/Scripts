Other:AddButton({
	Name = "Destroy Trespassing Areas",
	Callback = function()
		for i,v in pairs(game.Workspace.TrespassingAreas:GetChildren()) do
			v:Destroy()
		end
	end
})

Other:AddToggle({
	Name = "Detect Player Join",
	Default = false,
	Callback = function(Value)
		game.Players.PlayerAdded:Connect(function(plr)
			if Value == true then
				OrionLib:MakeNotification({
	Name = "Player Joined",
	Content = plr.Name.." Has joined.",
	Image = "rbxassetid://4483345998",
	Time = 3
})
			end
		end)
	end    
})

Other:AddToggle({
	Name = "Detect Player Leave",
	Default = false,
	Callback = function(Value)
		game.Players.PlayerRemoving:Connect(function(plr)
			if Value == true then
				OrionLib:MakeNotification({
	Name = "Player Left",
	Content = plr.Name.." Has rage quit",
	Image = "rbxassetid://4483345998",
	Time = 3
})
			end
		end)
	end    
})

OrionLib:MakeNotification({
	Name = "A heartwarming thanks.",
	Content = "Thanks for buying Anomic Revamped script!",
	Image = "rbxassetid://4483345998",
	Time = 5
})

OrionLib:Init()
