local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()



local Window = OrionLib:MakeWindow({Name = "YeN Anomic", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})






local Player = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local Combat = Window:MakeTab({
	Name = "Combat",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local Teleportation = Window:MakeTab({
	Name = "Teleportation",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local Vehicle = Window:MakeTab({
	Name = "Vehicle",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local AutoFarm = Window:MakeTab({
	Name = "AutoFarm",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local Other = Window:MakeTab({
	Name = "Other",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local OtherTP = Teleportation:AddSection({
	Name = "Other Teleportation"
})

local Teleports = Teleportation:AddSection({
	Name = "Waypoints"
})

Player:AddSlider({
	Name = "Walkspeed",	
	Min = 16,
	Max = 60,
	Default = 16,
	Color = Color3.fromRGB(0,0,255),
	Increment = 1,
	ValueName = "speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end
})

Player:AddSlider({
	Name = "Jumppower",
	Min = 50,
	Max = 450,
	Default = 50,
	Color = Color3.fromRGB(0,255,0),
	Increment = 1,
	ValueName = "jumppower",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})

Player:AddButton({
	Name = "FullBright (Glitchy)",
	Callback = function()
		game.Lighting.Brightness = 88
		game.Lighting.GlobalShadows = false
	end
})

Player:AddButton({
	Name = "Btools",
	Callback = function()
      	loadstring(game:HttpGet(('https://pastebin.com/raw/LETNPL0E')))()
  	end
})

OtherTP:AddTextbox({
	Name = "Teleport To Player",
	Default = "Player's Exact Username",
	TextDisappear = true,
	Callback = function(Value)
		if game.Players:FindFirstChild(Value) then
			local Oplr = game.Players:FindFirstChild(Value)
			local OplrPart = Oplr.Character.HumanoidRootPart
			
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(OplrPart.Position)
		end
	end	  
})

Player:AddButton({
	Name = "Animation Hub",
	Callback = function()
      	loadstring(game:HttpGet("https://pastebin.com/raw/BUjnjp4t", true))()
  	end
})

Combat:AddButton({
	Name = "Update ESP",
	Callback = function()
		for i,v in pairs(game.Players:GetChildren()) do
			local ESP = Instance.new("Highlight")

			ESP.Parent = v.Character
		end
	end
})

Combat:AddButton({
	Name = "Update Expanded Hitboxes",
	Callback = function()
		for i,v in pairs(game.Workspace.PlayerCharacters:GetDescendants()) do
			if v.Name == "Head" then
				v.Size = Vector3.new(6, 3, 3)
				v.Transparency = .5
			end
		end
	end
})

Combat:AddButton({
	Name = "Aimbot/Silent Aim",
	Callback = function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/Stefanuk12/Aiming/main/Examples/UniversalSilentAim.lua')))()
	end
})

OtherTP:AddButton({
	Name = "Teleport to Random Player Drop",
	Callback = function()
		for i,v in pairs(game.Workspace.Drops:GetChildren()) do
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.MainPart.Position)
		end
	end
})

Teleports:AddButton({
	Name = "Ridvine Hideout",
	Callback = function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(651.742, 7.326, 1290.64)
	end
})

