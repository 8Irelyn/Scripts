local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "YeN Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Scripts = Window:MakeTab({
	Name = "Scripts",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

Scripts:AddButton({
	Name = "YeN Universal",
	Callback = function()
      	loadstring(game:HttpGet(('https://raw.githubusercontent.com/8Irelyn/Scripts/main/YeN%20Universal')))()
  	end
})

Scripts:AddButton({
	Name = "Rainbow Friends",
	Callback = function()
      	loadstring(game:HttpGet(('https://raw.githubusercontent.com/8Irelyn/Scripts/main/Project%20LuaZ%20(Rainbow%20Friends)')))()
  	end
})






OrionLib:Init()
