-- CTRL A  Will Highlight everything. Then  CTRL C  and then  CTRL V  in Executor
-- Join us in our newly growing community:  https://discord.gg/QyQAqQss2R

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()


local Window = OrionLib:MakeWindow({Name = "Project LuaZ", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})



local Credits = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})
local Main = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local Teleports = Window:MakeTab({
	Name = "Teleports",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

local Settings = Window:MakeTab({
	Name = "Settings",
	Icon = "rbxassetid://0",
	PremiumOnly = false
})

Main:AddButton({
	Name = "ESP",
	Callback = function()
      	for i,v in pairs(game.Workspace.Monsters:GetDescendants()) do
			local highlight = Instance.new("Highlight")
			
			highlight.Parent = v
			highlight.Adornee = v
		end
  	end
})

local AreaOne = Main:AddSection({
	Name = "Area 1 (Blue)"
})

local AreaTwo = Main:AddSection({
	Name = "Area 2 (Blue & Green)"
})

local AreaThree = Main:AddSection({
	Name = "Area 3 (Blue & Green & Purple & Orange)"
})

local AreaFour = Main:AddSection({
	Name = "Area 4 (All Monsters + No Light)"
})

local Finale = Main:AddSection({
	Name = "Area 5 (FINALE)"
})

local MainTeleports = Teleports:AddSection({
	Name = "Main Areas"
})

Credits:AddLabel("Thanks for using my script!")
Credits:AddLabel("Current Developers --")
Credits:AddLabel("- YeN")


