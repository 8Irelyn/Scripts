AreaOne:AddButton({
	Name = "Highlight Blocks",
	Callback = function()
      	for i,v in pairs(game.Workspace:GetChildren()) do
      		if v.Name:find("Block") then
      			local Highlight = Instance.new("Highlight")
      			
      			Highlight:Clone().Parent = v
			end
		end
  	end
})


