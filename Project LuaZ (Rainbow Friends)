AreaOne:AddButton({
	Name = "Highlight Blocks",
	Callback = function()
      	for i,v in pairs(game.Workspace:GetChildren()) do
      		if v.Name:find("Block") then
      			local Highlight = Instance.new("Highlight")
      			
      			Highlight:Clone().Parent = v
			end
		end
  	end
})

AreaTwo:AddButton({
	Name = "Highlight Food Packs",
	Callback = function()
      	for i,v in pairs(game.Workspace:GetDescendants()) do
      		if v:IsA("MeshPart") and v.Parent:FindFirstChild("TouchTrigger") then
      			
      			local Highlight = Instance.new("Highlight")
      			
      			Highlight:Clone().Parent = v
      			
			end
		end
  	end
})


Main:AddButton({
	Name = "ESP",
	Callback = function()
      	for i,v in pairs(game.Workspace.Monsters:GetDescendants()) do
			local highlight = Instance.new("Highlight")
			
			highlight.Parent = v
			highlight.Adornee = v
		end
  	end
})

MainTeleports:AddButton({
	Name = "Spawn",
	Callback = function()
      	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(188.8,32,276.64965)
  	end
})

MainTeleports:AddButton({
	Name = "Theatre",
	Callback = function()
      	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(360.627,42,124.1853)
  	end
})

Settings:AddButton({
	Name = "DELETEGUI",
	Callback = function()
      	OrionLib:Destroy()
  	end
})

OrionLib:Init()
wait(3)

OrionLib:MakeNotification({
	Name = "Credits",
	Content = "Hey! Thanks for using my script! - Aiden",
	Image = "rbxassetid://6431340102",
	Time = 5
})
